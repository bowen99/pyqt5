    def people(self):
        app = Application().start(r"C:\Program Files (x86)\Tencent\WeChat\WeChat.exe")
        # pyperclip.copy("C:\Program Files (x86)\Tencent\WeChat\WeChat.exe")
        app = Application().connect(path="C:\Program Files (x86)\Tencent\WeChat\WeChat.exe")
        send_keys('^f')
        pyperclip.copy(self.lineEdit.text())
        send_keys('^v')
        time.sleep(1)
        pyautogui.press('enter')
        time.sleep(1)
        while True:
            pyperclip.copy(self.lineEdit_2.text())
            send_keys('^v')
            pyautogui.press('enter')
            print("开始轰炸！！！！")

